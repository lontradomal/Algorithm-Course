algoritmo "agecalcu"
var
    ano_atual, ano_nasci, age : inteiro
inicio
        Escreva ("What year are we in? ")
        Leia (ano_atual)
        Escreva ("What year have I born? ")
        Leia (ano_nasci)
        idade <- ano_atual -  ano_nasci
        Escreva ("My age should be: ", age)
fimalgoritmo